## command messages
terra121.commands.tpll.usage=/tpll <latitude> <longitude> [altitude] [player]
terra121.commands.dataset.usage=/dataset [dataset folder path]

terra121.commands.terra.usage=/terra
terra121.commands.terraadmin.usage=/terraadmin

terra121.commands.terra.gensettings=Generator Settings:
terra121.commands.terra.latlon=lat lon: %.7f %.7f
terra121.commands.terra.xy=x y: %.7f %.7f
terra121.commands.terra.environment=%.1f °C; %.1f mm/yr; soil id: %d
terra121.commands.terra.tissot= ~%.5f blocks/meter ± %.5f°
terra121.commands.terra.rstwtr=Water generation of Region ( %d, %d ) restored to default (This will only show up on newly generated chunks)
terra121.commands.terra.invwtr=Water generation of Region ( %d, %d ) inverted (This will only show up on newly generated chunks)

terra121.commands.overpass.usage=/terraadmin overpass <status|list|default|fallback>
terra121.commands.overpass.list.default=Default:  %s
terra121.commands.overpass.list.fallback=Fallback: %s
terra121.commands.overpass.list.nofallback=No fallback endpoint is set in the config
terra121.commands.overpass.set.default=Now using the default endpoint
terra121.commands.overpass.set.fallback=Now using the fallback endpoint
terra121.commands.overpass.status.default=Currently using default Overpass endpoint: %s
terra121.commands.overpass.status.fallback=Currently using fallback Overpass endpoint: %s

terra121.commands.terra.title=Terra++ Commands
terra121.commands.terraadmin.title=Terra++ Admin Commands

